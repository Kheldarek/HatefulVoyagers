@model biuro.Uzytkownik

@{
    ViewBag.Title = "Logowanie";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)
    @Html.LabelFor(m => m.Login)<br />
    @Html.TextBoxFor(m => m.Login)<br />
    @Html.LabelFor(m => m.Haslo)<br />
    @Html.PasswordFor(m => m.Haslo)<br />
    <input type="submit" value="Zaloguj" /><br />
    @Html.ActionLink("Zarejestruj się", "Register", "Account")
}

